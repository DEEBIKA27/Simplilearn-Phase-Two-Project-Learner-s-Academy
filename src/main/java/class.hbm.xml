<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
	<class name="com.simplilearn.entity.Classes" table="classes">
	<id name="section" type="int" column="Section" >
			<generator class ="increment"></generator>
		</id> 
		<property name="subjectName" type="string" column="subjectName"></property>
		<property name="teacherName" type="string" column="teacherName"></property>
	</class>
</hibernate-mapping>